{
  "name": "nebula-sdk-ts",
  "version": "0.1.4",
  "description": "TypeScript SDK for the Nebula",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build_and_pack": "rm -rf dist && npm run build && npm pack",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Annany2002/nebula-sdk-ts.git"
  },
  "keywords": [
    "sdk",
    "baas",
    "nebula",
    "api",
    "typescript"
  ],
  "author": "Annany Vishwakarma annivish2002@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Annany2002/nebula-sdk-ts/issues"
  },
  "homepage": "https://github.com/Annany2002/nebula-sdk-ts#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
